{
    "app": {
      "input_encoder":  "generic_template",
      "encoder_delimiter": "#",
      "output_decoder": "csv",
      "template":       "inputs/src.template",
      "input_filename": "src_input.nml"
    },
    "params": {
      "AMP_ION": {"type": "real", "min": "0.0", "max": "2.0", "default": "1.0"},
      "POS_ION": {"type": "real", "min": "0.0", "max": "2.0", "default": "1.0"},
      "WIDTH_ION": {"type": "real", "min": "0.0", "max": "2.0", "default": "1.0"},
      "out_file": {"type": "str", "default": "src_output.csv"}
    }
}
