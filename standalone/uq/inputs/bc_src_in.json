{
    "app": {
      "input_encoder":  "generic_template",
      "encoder_delimiter": "#",
      "output_decoder": "csv",
      "template":       "inputs/bc_src.template",
      "input_filename": "bc_src_input.nml"
    },
    "params": {
      "Te_boundary": {"type": "real", "min": "0.0", "max": "1000.0", "default": "100.0"},
      "Ti_boundary": {"type": "real", "min": "0.0", "max": "1000.0", "default": "100.0"},
      "WTOT_el": {"type": "real", "min": "0.0", "max": "2.E6", "default": "1.E6"},
      "RHEAT_el": {"type": "real", "min": "0.0", "max": "0.5", "default": "0.2"},
      "FWHEAT_el": {"type": "real", "min": "0.0", "max": "0.5", "default": "0.2"},
      "out_file": {"type": "str", "default": "bc_src_output.csv"}
    }
}
