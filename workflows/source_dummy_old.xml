 <parameters>

  <currents>

     <JNITOT>            1.E5      </JNITOT>      <!-- Total noninductive current [A] -->
     <RCURR>             0.5       </RCURR>       <!-- Position of the current profile maximum [m] -->
     <FWCURR>            0.2       </FWCURR>      <!-- Full width at the halth maximum of current profile [m] -->

  </currents>


   <electrons>

     <heating_el>
        <WTOT_el>        1.E6      </WTOT_el>     <!-- Total heating power [W] -->
        <RHEAT_el>       0.5       </RHEAT_el>    <!-- Position of the heating profile maximum [m] -->
        <FWHEAT_el>      0.2       </FWHEAT_el>   <!-- Full width at the halth maximum of heating profile [m] -->
     </heating_el>

 
      <particles_el>
         <STOT_el>       1.E21     </STOT_el>     <!-- Injection rate [s^-1] -->
         <RPART_el>      0.0       </RPART_el>    <!-- Position of the source profile maximum [m] -->
         <FWPART_el>     1.0       </FWPART_el>   <!-- Full width at the halth maximum of source profile [m] -->
      </particles_el>

   </electrons>

 
   <ions>
      <composition>
         <AMN>           2.0       </AMN>         <!-- Total heating power [W] -->
         <ZN>            1.0       </ZN>          <!-- Position of the heating profile maximum [m] -->
         <ZION>          1.0       </ZION>        <!-- fraction of power going in different components [-] -->
      </composition>

 
      <heating>
         <WTOT>          1.E6      </WTOT>        <!-- Total heating power [W] -->
         <RHEAT>         0.0       </RHEAT>       <!-- Position of the heating profile maximum [m] -->
         <FWHEAT>        0.5       </FWHEAT>      <!-- Full width at the halth maximum of heating profile [m] -->
      </heating>

 
      <particles>
         <STOT>          1.E21     </STOT>        <!-- Injection rate [s^-1] -->
         <RPART>         0.0       </RPART>       <!-- Position of the source profile maximum [m] -->
         <FWPART>        1.0       </FWPART>      <!-- Full width at the halth maximum of source profile [m] -->
      </particles>

 
      <momentum>
         <UTOT>          0.E0      </UTOT>        <!-- Total momentum [kg*m^2*s^-1] -->
         <RMOM>          0.0       </RMOM>        <!-- Position of the momentum profile maximum [m] -->
         <FWMOM>         1.0       </FWMOM>       <!-- Full width at the halth maximum of momentum profile [m] -->
      </momentum>

   </ions>


 </parameters>
 
