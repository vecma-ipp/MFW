<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:NS1="ImportNamespace" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:element name="parameters">
    <xs:complexType>
      <xs:all>
        <xs:element name="COCOS_verbose">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="cocos_in" default="13">
                <xs:simpleType>
                  <xs:restriction base="xs:integer">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="18"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="cocos_out" default="13">
                <xs:simpleType>
                  <xs:restriction base="xs:int">
                    <xs:minInclusive value="1"/>
                    <xs:maxInclusive value="18"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="nverbose" type="xs:int" default="1">
                <xs:annotation>
                  <xs:documentation>-1 to write nothing, 0 almost nothing, 1 minimal, 3 maximal </xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="choice_of_input_profiles">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="first_source">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="pprime">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nppfun" fixed="4" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="pressure">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nppfun" fixed="8" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="pprime_parametric">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nppfun" default="2" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <xs:element name="second_source">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="Istar_jphi">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nfunc" fixed="4" type="xs:int"/>
                          <xs:element name="nsttp" fixed="2" type="xs:int"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ffprime">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nfunc" fixed="4" type="xs:int"/>
                          <xs:element name="nsttp" fixed="1" type="xs:int"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Iparallel_chease">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nfunc" fixed="4" type="xs:int"/>
                          <xs:element name="nsttp" fixed="3" type="xs:int"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="jparallel_jB_over_B0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nfunc" fixed="4" type="xs:int"/>
                          <xs:element name="nsttp" fixed="4" type="xs:int"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="parametric">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nfunc" default="2" type="xs:int"/>
                          <xs:element name="nsttp" default="2" type="xs:int"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="radial_input_mesh">
          <xs:annotation>
            <xs:documentation>Note: rho_tor_norm not valid for ffprime at this stage </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="rho_pol_norm_standard_chease">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nfunrho" fixed="0" type="xs:integer"/>
                    <xs:element name="nrhomesh" fixed="0" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="rho_tor_norm">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nfunrho" fixed="1" type="xs:integer"/>
                    <xs:element name="nrhomesh" fixed="1" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="mapping_mesh_output">
          <xs:annotation>
            <xs:documentation>In coord_sys in CPO, decides on jacobian ~ R^NER grad_psi^NEGP </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="equal_arc_length">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="NER" fixed="1" type="xs:integer"/>
                    <xs:element name="NEGP" fixed="-1" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="straight-field-line">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="NER" fixed="2" type="xs:integer"/>
                    <xs:element name="NEGP" fixed="0" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Hamada">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="NER" fixed="0" type="xs:integer"/>
                    <xs:element name="NEGP" fixed="0" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="other_choice">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="NER" default="1" type="xs:integer"/>
                    <xs:element name="NEGP" default="-1" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="rescaling_options">
          <xs:annotation>
            <xs:documentation>Should choose no rescaling within transport loop</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="no_rescaling">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ncscal" fixed="4" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="specify_q_value_at_csspec">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ncscal" fixed="1" type="xs:integer"/>
                    <xs:element name="csspec" default="0." type="xs:float"/>
                    <xs:element name="qspec" default="0.95" type="xs:float"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="specify_tot_current_in_chease_units">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ncscal" fixed="2" type="xs:integer"/>
                    <xs:element name="currt" default="0.5" type="xs:float"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="any_choice">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ncscal" default="1" type="xs:integer"/>
                    <xs:element name="currt" default="0.5" type="xs:float"/>
                    <xs:element name="csspec" default="0." type="xs:float"/>
                    <xs:element name="qspec" default="0.95" type="xs:float"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="mesh_characteristics">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="number_intervals_equil_mesh">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="very_small">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NS" fixed="24" type="xs:integer"/>
                          <xs:element name="NT" fixed="24" type="xs:integer"/>
                          <xs:element name="epslon" type="xs:float" default="1.0e-6"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="small">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NS" fixed="30" type="xs:integer"/>
                          <xs:element name="NT" fixed="30" type="xs:integer"/>
                          <xs:element name="epslon" type="xs:float" default="1.0e-7"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="standard">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NS" fixed="40" type="xs:integer"/>
                          <xs:element name="NT" fixed="40" type="xs:integer"/>
                          <xs:element name="epslon" type="xs:float" default="1.0e-8"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="large">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NS" fixed="60" type="xs:integer"/>
                          <xs:element name="NT" fixed="60" type="xs:integer"/>
                          <xs:element name="epslon" type="xs:float" default="1.0e-10"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="other_choice">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NS" default="40" type="xs:integer"/>
                          <xs:element name="NT" default="40" type="xs:integer"/>
                          <xs:element name="epslon" type="xs:float" default="1.0e-8"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <xs:element name="number_intervals_mapping_mesh">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="small">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="100" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="100" type="xs:integer"/>
                          <xs:element name="NISO" fixed="100" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="very_small">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="80" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="80" type="xs:integer"/>
                          <xs:element name="NISO" fixed="80" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="fast_standard">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="120" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="120" type="xs:integer"/>
                          <xs:element name="NISO" fixed="120" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="standard">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="180" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="180" type="xs:integer"/>
                          <xs:element name="NISO" fixed="180" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="large">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="256" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="256" type="xs:integer"/>
                          <xs:element name="NISO" fixed="256" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="other_choice">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NPSI" fixed="140" type="xs:integer"/>
                          <xs:element name="NCHI" fixed="140" type="xs:integer"/>
                          <xs:element name="NISO" fixed="140" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <xs:element name="number_eqdskRZ_mesh_and_box">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="small">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nrbox" fixed="101" type="xs:integer"/>
                          <xs:element name="nzbox" fixed="101" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="standard">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nrbox" fixed="129" type="xs:integer"/>
                          <xs:element name="nzbox" fixed="129" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="large">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nrbox" fixed="257" type="xs:integer"/>
                          <xs:element name="nzbox" fixed="257" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="other_choice">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nrbox" default="65" type="xs:integer"/>
                          <xs:element name="nzbox" default="65" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
              <xs:element name="various_mesh_packing">
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="no_packing">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="nmesha" fixed="0" type="xs:integer"/>
                          <xs:element name="ndifps" fixed="0" type="xs:integer"/>
                          <xs:element name="nmeshb" fixed="0" type="xs:integer"/>
                          <xs:element name="nmeshc" fixed="0" type="xs:integer"/>
                          <xs:element name="nmeshd" fixed="0" type="xs:integer"/>
                          <xs:element name="ndift"  fixed="0" type="xs:integer"/>
                          <xs:element name="nmeshe" fixed="0" type="xs:integer"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="standard">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:choice>
                            <xs:element name="nmesha_stability_s_mesh_packing_on_s_values">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmesha" default="1" type="xs:integer"/>
                                  <xs:element name="npoida" default="2" type="xs:integer"/>
                                  <xs:element name="aplace" default="0.96 1.0" type="FloatList"/>
                                  <xs:element name="awidth" default="0.07 0.02" type="FloatList"/>
                                  <xs:element name="solpda" default="0.70" type="xs:float"/>
                                  <xs:element name="ndifps" default="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshb_intermediate_s_mesh_nopacking">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshb" fixed="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmesha_stability_s_packing_on_q_values">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmesha" default="2" type="xs:integer"/>
                                  <xs:element name="npoidq" default="2" type="xs:integer"/>
                                  <xs:element name="qplace" default="0.96 1.0" type="FloatList"/>
                                  <xs:element name="qwidth" default="0.07 0.02" type="FloatList"/>
                                  <xs:element name="solpda" default="0.70" type="xs:float"/>
                                  <xs:element name="ndifps" default="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmesha_stability_s_rhogen_equidistant">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="ndifps" default="1" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                          <xs:choice>
                            <xs:element name="nmeshb_intermediate_s_mesh_nopacking">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshb" fixed="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshb_intermediate_s_mesh">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshb" default="0" type="xs:integer">
                                    <xs:annotation>
                                      <xs:documentation>set nmeshb=1 to enable packing</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="npoidb" default="2" type="xs:integer"/>
                                  <xs:element name="bplace" default="0.96 1.0" type="FloatList"/>
                                  <xs:element name="bwidth" default="0.07 0.02" type="FloatList"/>
                                  <xs:element name="solpdb" default="0.70" type="xs:float"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                          <xs:choice>
                            <xs:element name="nmeshc_equil_sigma_mesh_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshc" default="1" type="xs:integer"/>
                                  <xs:element name="npoidc" default="2" type="xs:integer"/>
                                  <xs:element name="cplace" default=".95 .99 1.0" type="FloatList"/>
                                  <xs:element name="cwidth" default=".10 .02 .05" type="FloatList"/>
                                  <xs:element name="solpdc" default="0.70" type="xs:float"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshc_equil_sigma_mesh_no_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshc" fixed="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                          <xs:choice>
                            <xs:element name="nmeshd_equil_theta_mesh_auto_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshd" fixed="0" type="xs:integer"/>
                                  <xs:element name="ndift" default="2" type="xs:integer">
                                    <xs:annotation>
                                      <xs:documentation>ndift=1, equal pol. flux area; ndift=2, equal arc length</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshd_equil_theta_mesh_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshd" default="1" type="xs:integer"/>
                                  <xs:element name="npoidd" default="4" type="xs:integer"/>
                                  <xs:element name="dplace" default="-1.70 -1.70  1.70  1.70" type="FloatList"/>
                                  <xs:element name="dwidth" default=".18 .08 .18 .08" type="FloatList"/>
                                  <xs:element name="solpdd" default="0.50" type="xs:float"/>
                                  <xs:element name="ndift" default="0" type="xs:int"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshd_equil_chi_mesh_no_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshd" fixed="0" type="xs:integer"/>
                                  <xs:element name="ndift"  fixed="0" type="xs:int"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                          <xs:choice>
                            <xs:element name="nmeshe_chi_mesh_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshe" default="1" type="xs:integer"/>
                                  <xs:element name="npoide" default="4" type="xs:integer"/>
                                  <xs:element name="eplace" default="-1.70 -1.70  1.70  1.70" type="FloatList"/>
                                  <xs:element name="ewidth" default=".18 .08 .18 .08" type="FloatList"/>
                                  <xs:element name="solpde" default="0.50" type="xs:float"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="nmeshe_chi_mesh_no_packing">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="nmeshe" fixed="0" type="xs:integer"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="plasma_boundary">
          <xs:annotation>
            <xs:documentation>input for R,Z boundary (from array if from file or database, default)</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="from_input_RZ_array">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nsurf" fixed="6" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="analytical">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nsurf" default="2" type="xs:integer"/>
                    <xs:element name="aspct" default="0.32" type="xs:float"/>
                    <xs:element name="beans" default="0.0" type="xs:float"/>
                    <xs:element name="ceta" default="0.0" type="xs:float"/>
                    <xs:element name="cq0" default="0.95" type="xs:float"/>
                    <xs:element name="delta" default="0.0" type="xs:float"/>
                    <xs:element name="elong" default="1.7" type="xs:float"/>
                    <xs:element name="rc" default="1.0" type="xs:float"/>
                    <xs:element name="rnu" default="0.0" type="xs:float"/>
                    <xs:element name="rz0" default="0.0" type="xs:float"/>
                    <xs:element name="r0" default="1.0" type="xs:float"/>
                    <xs:element name="sgma" default="0.0" type="xs:float"/>
                    <xs:element name="theta0" default="0.0" type="xs:float"/>
                    <xs:element name="triang" default="0.5" type="xs:float"/>
                    <xs:element name="triplt" default="0.0" type="xs:float"/>
                    <xs:element name="xi" default="0.0" type="xs:float"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="nideal_output_mapping_choice">
          <xs:annotation>
            <xs:documentation>type of output calculations for other codes, nideal=6 for standard eqdsk or kepler</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="standard_kepler_eqdsk">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nideal" fixed="6" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="MARS">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="nideal" fixed="0" type="xs:integer"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="LION">
                <xs:complexType>
