#!/usr/bin/tcsh -fe

set DIR=fc2k

foreach ACTOR (`ls $DIR/*.xml`)
    echo " "
    echo "Processing file: $ACTOR"
    echo $ACTOR
    set ACTOR_NAME=`basename $ACTOR .xml`
    rmactor $ACTOR_NAME
    $FC2K/bin/fc2k -kepler -ual -nocompile $ACTOR
end

echo " "
echo "Processing file: $ACTOR"
echo $ACTOR
rmactor $ACTOR_NAME
$FC2K/bin/fc2k -kepler -ual  $ACTOR
