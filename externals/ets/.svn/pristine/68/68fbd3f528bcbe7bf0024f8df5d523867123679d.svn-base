<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!-- document element -->
  <xs:element name="parameters">
    <xs:complexType>
      <xs:all>
        <xs:element ref="input" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <!-- input parameters -->
  <xs:element name="input">
    <xs:complexType>
      <xs:all>
         <xs:element ref="functions" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="functions" type="StringList"/>

</xs:schema>
