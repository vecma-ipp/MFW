 <parameters>

 <!-- multipliers for B/GB parts of total coefficients  -->

     <C_GB_TE>         1     </C_GB_TE>     <!-- multiplier for electron heat transport contribution from GB-->
     <C_GB_TI>         2     </C_GB_TI>     <!-- multiplier for ion  heat transport contribution from GB-->

     <C_B_TE>          1     </C_B_TE>      <!-- multiplier for electron heat transport contribution from Bohm -->
     <C_B_TI>          1     </C_B_TI>      <!-- multiplier for ion  heat transport contribution from Bohm -->


 </parameters>
 
