foreach s (1 2 3)
  foreach t (1e-1 1e-2 1e-3 1e-4)
    foreach r (5 9 17 33 65 129 257 513 1025)
      set n=`echo $t | awk '{print 1/$1}'`
      set CASE=${s}_${t}_${r}_${n}
      set OUT=`echo $n | awk '{printf "OUT%05i.DAT\n",$1}'`
      [ -e $CASE/data/OUTPUT/$OUT ] || echo $CASE
    end
  end
end

