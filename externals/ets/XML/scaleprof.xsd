<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!-- document element -->
  <xs:element name="parameters">
    <xs:annotation>
      <xs:documentation>
        GLOBAL: parameters for source combiner
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="multipliers" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <!-- multipliers parameters -->
  <xs:element name="multipliers">
    <xs:annotation>
      <xs:documentation>
        BLOCK: specify multipliers
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="S_psi" maxOccurs="1"/>
        <xs:element ref="S_jpar" maxOccurs="1"/>
        <xs:element ref="S_ne" maxOccurs="1"/>
        <xs:element ref="S_te" maxOccurs="1"/>
        <xs:element ref="S_ni" maxOccurs="1"/>
        <xs:element ref="S_ti" maxOccurs="1"/>
        <xs:element ref="S_vtor" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>


  <xs:element name="S_psi" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_jpar" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_ne" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_te" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_ni" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_ti" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="S_vtor" type="xs:float">			
    <xs:annotation>
      <xs:documentation>
        output frequency
      </xs:documentation>
    </xs:annotation>
  </xs:element>



</xs:schema>
