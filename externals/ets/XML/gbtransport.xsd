<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <!-- document element -->
  <xs:element name="parameters">
    <xs:annotation>
      <xs:documentation>
        GLOBAL: multipliers for B/GB parts of total coefficients
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:all>
        <xs:element ref="C_GB_TE" maxOccurs="1"/>
        <xs:element ref="C_GB_TI" maxOccurs="1"/>
        <xs:element ref="C_B_TE" maxOccurs="1"/>
        <xs:element ref="C_B_TI" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="C_GB_TE" type="float">
    <xs:annotation>
      <xs:documentation>
        multiplier for GB part of electron transport
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="C_GB_TI" type="float">
    <xs:annotation>
      <xs:documentation>
        multiplier for GB part of ion transport 
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="C_B_TE" type="float">
    <xs:annotation>
      <xs:documentation>
        multiplier for Bohm part of electron transport 
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="C_B_TI" type="float">
    <xs:annotation>
      <xs:documentation>
        multiplier for  Bohm part of ion transport
      </xs:documentation>
    </xs:annotation>
  </xs:element>

</xs:schema>
