SUBROUTINE RVAR2(KN1,PV1,KN2,PV2)
  !        ---------------------------------
  !
  ! U.21   PRINT NAME AND VALUE OF TWO REAL VARIABLES             
  !
  use prec_const
  implicit none
  !
  CHARACTER*(*)              :: kn1
  CHARACTER*(*)              :: kn2
  REAL(RKIND)                :: pv1
  REAL(RKIND)                :: pv2
  !
  WRITE (6,9900) KN1,PV1,KN2,PV2
  !
  RETURN
9900 FORMAT(/,1X,A,' =',1PE17.8,T36,A,' =',1PE17.8)
END SUBROUTINE RVAR2
