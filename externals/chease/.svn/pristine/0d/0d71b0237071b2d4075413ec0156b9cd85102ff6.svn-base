&nlist                                 ! Name of namelist 
  string_var    = 'A simple string',   ! string
  int_var =14, real_var= -0.5 , double_var = 1.D4
  double_array=1.20, .272E1,
               314.D-2, 24
  real_array = 0, 1,                   ! we _can_ put a comment here
               2, 3.e0,
/
